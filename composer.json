{
    "name": "effiana/cron-bundle",
    "type": "symfony-bundle",
    "description": "Symfony cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Labudzinski",
            "email": "dominik@labudzinski.com"
        },
        {
            "name": "Dries De Peuter",
            "email": "dries@nousefreak.be"
        }
    ],
    "require": {
        "php": "^7.2",
        "effiana/cron": "^2.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-bundle": "*",
        "effiana/doctrine-cache-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "^1.3 | ^2.1",
        "doctrine/orm": "*",
        "symfony/framework-bundle": "^4.2 | ^5.0",
        "effiana/migration-bundle": "dev-master"
    },
    "autoload": {
        "psr-4" : {
            "Effiana\\CronBundle\\" : ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "Tests/" },
        "files": [ "Tests/Fixtures/App/app/AppKernel.php" ]
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    }
}
