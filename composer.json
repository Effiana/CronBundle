{
    "name": "effiana/cron-bundle",
    "type": "symfony-bundle",
    "description": "Symfony cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Labudzinski",
            "email": "dominik@labudzinski.com"
        },
        {
            "name": "Dries De Peuter",
            "email": "dries@nousefreak.be"
        }
    ],
    "require": {
        "php": "7.2.*",
        "effiana/cron": "^2.0",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/orm": "~2.4,>=2.4.5",
        "symfony/symfony": "^4.2"
    },
    "autoload": {
        "psr-4" : {
            "Effiana\\CronBundle\\" : ""
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "Tests/" },
        "files": [ "Tests/Fixtures/App/app/AppKernel.php" ]
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    }
}
